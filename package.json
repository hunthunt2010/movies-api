{
  "name": "aetna-code-test",
  "version": "1.0.0",
  "description": "",
  "main": "./src/server.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "node ./src/server.js",
    "start:dev": "nodemon ./src/server.js",
    "test": "jest --coverage --runInBand --forceExit",
    "test:watch": "jest --watch --runInBand"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/server.js"
    ]
  },
  "repository": {
    "type": "git"
  },
  "dependencies": {
    "knex": "^0.21.12",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-connect": "^2.1.0",
    "koa-router": "^10.0.0",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^1.17.1",
    "supertest": "^6.0.1"
  }
}
